      name: agencyID
      in: path
      description: The agency maintaining the artefact used to generate the schema
        to be returned.
      required: true
      style: simple
      explode: false
      schema:
        pattern: "^[A-Za-z][A-Za-z\\d_-]*(\\.[A-Za-z][A-Za-z\\d_-]*)*$"
        type: string
